<?xml version="1.0"?>
<Container version="2">
  <Name>WakeOnMc</Name>
  <Repository>ghcr.io/yannikmg/wakeonlan:latest</Repository>
  <Registry>https://github.com/YannikMG/wakeonmc</Registry>
  <Network>host</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://github.com/YannikMG/wakeonmc</Project>
  <Overview>This is a simple redirect page i made for a friend of mine in node.js that uses WakeOnLan to that his PC can power up and redirect it to the Pterodactyl landing page</Overview>
  <Category>Productivity: Tools:</Category>
  <WebUI>http://[IP]:[PORT:3000]/</WebUI>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/YannikMG/wakeonmc/master/unraid-template/wakeonmc.png</Icon>
  <ExtraParams>--restart=unless-stopped</ExtraParams>
  <PostArgs/>
  <CPUset/>
  <DateInstalled/>
  <DonateText/>
  <DonateLink/>
  <Description>This is a simple redirect page i made for a friend of mine in node.js that uses WakeOnLan to that his PC can power up and redirect it to the Pterodactyl landing page</Description>
  <Data/>
  <Environment>
    <Variable>
      <Value>3005</Value>
      <Name>PORT</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>10.10.0.255</Value>
      <Name>BROADCAST</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>00:00:00:00:00:00</Value>
      <Name>MACADDRESS</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>https://www.google.com/</Value>
      <Name>REDIRECTWEBPAGE</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="Web Port" Target="PORT" Default="3005" Mode="" Description="Container Port: 3005" Type="Variable" Display="always" Required="true" Mask="false">3005</Config>
  <Config Name="Broadcast IP" Target="BROADCAST" Default="10.10.0.255" Mode="" Description="Container Variable: BROADCAST" Type="Variable" Display="always" Required="true" Mask="false">10.10.0.255</Config>
  <Config Name="Mac Address of PC" Target="MACADDRESS" Default="00:00:00:00:00:00" Mode="" Description="Container Variable: MACADDRESS" Type="Variable" Display="always" Required="true" Mask="false">00:00:00:00:00:00</Config>
  <Config Name="Redirect to Page" Target="REDIRECTWEBPAGE" Default="https://www.google.com/" Mode="" Description="Container Variable: REDIRECTWEBPAGE" Type="Variable" Display="always" Required="true" Mask="false">https://www.google.com/</Config>
</Container>
